import jeliot.io.*;

public class Train {
    //Attributes
        private String train;
        private int passengers;
        private boolean isExp;
    
    //Constractors
    public Train (String train , int passengers , boolean isExp ) {
        this.train = train;
        this.passengers = passengers;
        this.isExp = isExp;
    }
    
    public Train() {
        this.train = "rishonTelAviv";
        this.passengers = 200;
        this.isExp = true;
    }
    
     //Setters&Getters
     public void setString (String train) {
         this.train = train;
     }
     
     public double getString() {
         return this.train;
     }
     
     public void setInt (int passengers) {
         this.passengers = passengers;
     }
     
     public double getInt () {
         return this.passengers;
     }
     
     public void setBoolean (boolean isExp) {
         this.isExp = isExp;
     }
     public double getBoolean() {
         return this.isExp;
         }
    
}

public class Train2 {
    public static void main() {
    
            String train = "rishonTelAviv";
            int passengers = 100;
            boolean isExp = true ;
            Train tra = new Train (train,passengers,isExp);import jeliot.io.*;

public class Train {
    //Attributes
        private String train;
        private int passengers;
        private boolean isExp;
    
    //Constractors
    public Train (String train , int passengers , boolean isExp ) {
        this.train = train;
        this.passengers = passengers;
        this.isExp = isExp;
    }
    
    public Train() {
        this.train = "rishonTelAviv";
        this.passengers = 200;
        this.isExp = true;
    }
    
     //Setters&Getters
     public void setString (String train) {
         this.train = train;
     }
     
     public double getString() {
         return this.train;
     }
     
     public void setInt (int passengers) {
         this.passengers = passengers;
     }
     
     public double getInt () {
         return this.passengers;
     }
     
     public void setBoolean (boolean isExp) {
         this.isExp = isExp;
     }
     public double getBoolean() {
         return this.isExp;
         }
    
}

public class Train2 {
    public static void main() {
    
            String train = "rishonTelAviv";
            int passengers = 100;
            boolean isExp = true ;
            Train tra = new Train (train,passengers,isExp);
    }
}

